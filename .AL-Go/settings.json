{
  "$schema": "https://raw.githubusercontent.com/microsoft/AL-Go-Actions/v7.2/settings.schema.json",
  "country": "us",
  "appFolders": ["."],
  "testFolders": [],
  "bcptTestFolders": [],
  "VersioningStrategy": 16,
  "repoVersion": "1.0",
  "type": "PTE",
  "enableCodeCop": true,
  "enableAppSourceCop": true,
  "enablePerTenantExtensionCop": true,
  "enableUICop": true,
  "failOn": "warning",
  "doNotBuildTests": false,
  "doNotRunTests": false,
  "installTestRunner": true,
  "installTestFramework": true,
  "installTestLibraries": true,
  "installPerformanceToolkit": true,
  "gitHubRunner": "windows-latest",
  "gitHubRunnerShell": "powershell",
  "CICDPushBranches": [ "main", "develop", "release/*" ],
  "CICDPullRequestBranches": [ "main" ],
  "applicationInsightsConnectionString": "",
  "authContextSecrets": {
    "scopes": "https://api.businesscentral.dynamics.com/.default"
  }
}
